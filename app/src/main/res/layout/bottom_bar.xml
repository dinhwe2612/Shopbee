            android:text="Favorites"
            style="@style/Gray_Regular_10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

        </TextView>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
